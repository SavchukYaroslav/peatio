<div class="row">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <span><%= t('.markets') %></span>
    </div>
    <div class="panel-body">
      <%= table_for @markets, class: 'table table-condensed table-hover' do |t| %>
        <% t.column :id, class: 'col-xs-1' %>
      <% end %>

      <%= paginate @markets %>
    </div>
  </div>
</div>
